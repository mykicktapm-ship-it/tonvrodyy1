# Environment variables for the frontend (Vite)
# URL where your tonconnect manifest is publicly available.
VITE_TONCONNECT_MANIFEST=https://yourdomain.com/tonconnect-manifest.json

# Salt used to derive a unique app user ID from the Telegram user ID.
VITE_APP_SALT=your_unique_app_salt

# Base URL of the backend API (e.g. https://api.yourdomain.com). Used for network requests.
VITE_BACKEND_URL=https://yourbackend.com
VITE_TELEGRAM_BOT_USERNAME=tonrodybuild_bot
