{
  "name": "tonvrodyy12",
  "version": "1.0.0",
  "private": true,
  "description": "TON WebApp monorepo for Telegram Mini App with TON Connect, Chakra UI and 3D glass plasma design",
  "workspaces": [
    "frontend",
    "backend"
  ],
  "engines": {
    "node": ">=20"
  },
  "packageManager": "pnpm@9.7.0",
  "scripts": {
    "dev": "pnpm -r run dev",
    "build": "pnpm -r run build",
    "start": "pnpm --filter backend run start",
    "install:stack": "node install.js"
  },
  "keywords": [
    "ton",
    "telegram",
    "mini-app",
    "vite",
    "react",
    "typescript",
    "chakra-ui",
    "three.js",
    "glassmorphism"
  ],
  "license": "MIT",
  "dependencies": {
    "@chakra-ui/react": "2.8.2",
    "@emotion/react": "11.11.1",
    "@emotion/styled": "11.11.0",
    "@react-three/drei": "9.97.0",
    "@react-three/fiber": "8.15.10",
    "@react-three/postprocessing": "^2.16.5",
    "@react-three/rapier": "^2.1.0",
    "@ton/core": "0.57.0",
    "@tonconnect/ui-react": "2.3.1",
    "framer-motion": "11.0.6",
    "glsl-noise": "^0.0.0",
    "gsap": "^3.13.0",
    "leva": "^0.10.1",
    "postprocessing": "^6.34.1",
    "r3f-perf": "^7.2.3",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "stats.js": "^0.17.0",
    "three": "0.160.0",
    "three-custom-shader-material": "^6.4.0",
    "three-stdlib": "^2.36.0",
    "valtio": "^2.1.8"
  },
  "devDependencies": {
    "autoprefixer": "10.4.20",
    "postcss": "8.4.45",
    "tailwindcss": "3.4.5",
    "typescript": "5.3.3",
    "vite": "5.0.12"
  }
}
